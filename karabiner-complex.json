{
  "title": "Shell aka bindings",
  "rules": [
    {
      "description": "Return",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "modifiers": { "mandatory": ["right_control"] },
            "key_code": "m"
          },
          "to": [{ "repeat": true, "key_code": "return_or_enter" }]
        }
      ]
    },
    {
      "description": "Escape",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "modifiers": { "mandatory": ["right_control"] },
            "key_code": "spacebar"
          },
          "to": [{ "repeat": true, "key_code": "escape" }]
        }
      ]
    }
  ]
}

