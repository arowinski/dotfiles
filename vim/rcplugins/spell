Plug 'kamykn/spelunker.vim'

autocmd ColorScheme *
    \ highlight SpelunkerSpellBad cterm=underline ctermfg=247 gui=underline guifg=#eb4034 |
    \ highlight SpelunkerComplexOrCompoundWord cterm=underline ctermfg=NONE gui=underline guifg=NONE

nmap [s <Plug>(spelunker-jump-prev)
nmap ]s <Plug>(spelunker-jump-next)
nmap zs <Plug>(spelunker-correct-feeling-lucky)
nmap zS <Plug>(spelunker-correct-from-list)
nmap zg <Plug>(add-spelunker-good-nmap)
nmap zug <Plug>(undo-spelunker-good-nmap)

" vim:ft=vim
