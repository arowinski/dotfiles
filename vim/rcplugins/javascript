" JavaScript - syntax and indent, additionaly JSX support

Plug 'pangloss/vim-javascript', { 'for': 'javascript' }
Plug 'maxmellon/vim-jsx-pretty', { 'for': 'javascript' }

highlight def link jsxCloseString Label
" highlight def link jsxOpenPunct Label
highlight def link jsxComponentName Label

autocmd BufEnter *.es6 setf javascript

" vim:ft=vim
