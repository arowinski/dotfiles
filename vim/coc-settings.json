{
  "coc": { "preferences": { "snippetIndicator": "" } },
  "snippets": {
    "ultisnips": {
      "directories": ["UltiSnips", "plugged/vim-go/gosnippets/UltiSnips"]
    }
  },
  "suggest": { "noselect": true, "acceptSuggestionOnCommitCharacter": true },
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"]
    }
  }
}
